#*****application configuration***
server.port=8181
server.servlet.context-path=/prodos_resource_srvr

# *******DB Config*******
spring.jpa.show-sql=true

# *******Logging*******
logging.level.root=INFO
logging.level.org.springframework.security: INFO

# *******OAuth Config*******
security.oauth2.resource.userInfoUri=http://localhost:8282/prodos_auth_srvr/user
security.oauth2.client.accessTokenUri=http://localhost:8282/prodos_auth_srvr/oauth/token
security.oauth2.client.userAuthorizationUri=http://localhost:8282/prodos_auth_srvr/oauth/authorize
security.oauth2.client.clientId=magicbricks
security.oauth2.client.clientSecret=password

